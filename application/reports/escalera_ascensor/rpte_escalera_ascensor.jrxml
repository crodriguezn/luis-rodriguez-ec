	<queryString>
		<![CDATA[SELECT
	ea.r_date,
	ea.r_hour,
	ea.id_group_camera,
	ea.group_camera,
	ea.event,
	ea.camera,
	ea.total
FROM
	escalera_and_ascensor AS ea
WHERE
	1 = 1
	AND (ea.r_date BETWEEN '$P!{fecha_inicio}' AND '$P!{fecha_fin}')
	$P!{condicion}
ORDER BY
	ea.id_group_camera,
	ea.event,
	ea.camera,
	ea.r_date,
	ea.r_hour]]>
	</queryString>
