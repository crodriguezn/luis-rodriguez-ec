	<queryString>
		<![CDATA[SELECT
	t_.r_date,
	t_.r_hour,
	t_.id_group_camera,
	t_.group_camera,
	t_.event,
	t_.camera,
	t_.total
FROM
	torniquete AS t_
WHERE
	1 = 1
	AND (t_.r_date BETWEEN '$P!{fecha_inicio}' AND '$P!{fecha_fin}')
	$P!{condicion}
ORDER BY
	t_.id_group_camera,
	t_.event,
	t_.camera,
	t_.r_date,
	t_.r_hour]]>
	</queryString>
