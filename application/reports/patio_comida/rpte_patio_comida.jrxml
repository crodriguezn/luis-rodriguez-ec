	<queryString>
		<![CDATA[SELECT
	pc.r_date,
	pc.r_hour,
	pc.id_group_camera,
	pc.group_camera,
	pc.event,
	pc.camera,
	pc.total
FROM
	patios_comida AS pc
WHERE
	1 = 1
	AND (pc.r_date BETWEEN '$P!{fecha_inicio}' AND '$P!{fecha_fin}')
	$P!{condicion}
ORDER BY
	pc.id_group_camera,
	pc.event,
	pc.camera,
	pc.r_date,
	pc.r_hour]]>
	</queryString>
