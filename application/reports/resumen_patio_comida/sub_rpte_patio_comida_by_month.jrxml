	<queryString>
		<![CDATA[SELECT
	t_.year2,
	(
		CASE
		WHEN (t_.month2 = '1') THEN
			'ENERO'
		WHEN (t_.month2 = '2') THEN
			'FEBRERO'
		WHEN (t_.month2 = '3') THEN
			'MARZO'
		WHEN (t_.month2 = '4') THEN
			'ABRIL'
		WHEN (t_.month2 = '5') THEN
			'MAYO'
		WHEN (t_.month2 = '6') THEN
			'JUNIO'
		WHEN (t_.month2 = '7') THEN
			'JULIO'
		WHEN (t_.month2 = '8') THEN
			'AGOSTO'
		WHEN (t_.month2 = '9') THEN
			'SEPTIEMBRE'
		WHEN (t_.month2 = '10') THEN
			'OCTUBRE'
		WHEN (t_.month2 = '11') THEN
			'NOVIEMBRE'
		WHEN (t_.month2 = '12') THEN
			'DICIEMBRE'
		ELSE
			'NO FOUND'
		END
	) AS month2,
	t_.event,
	t_.group_camera,
	SUM (t_.total) AS total,
	round(AVG(t_.total)) AS promedio,
	MIN (t_.total) AS minimo,
	MAX (t_.total) AS maximo
FROM
	(
		SELECT
			(
				EXTRACT (YEAR FROM d_a_t.r_date)
			) AS year2,
			(
				EXTRACT (MONTH FROM d_a_t.r_date)
			) AS month2,
			d_a_t.group_camera,
			d_a_t.event,
			SUM (d_a_t.total) AS total
		FROM
			(
				SELECT
					a_t.r_date,
					a_t.r_hour,
					a_t.event,
					a_t.group_camera,
					a_t.camera,
					SUM (a_t.total) AS total
				FROM
					patios_comida AS a_t
				WHERE
					1 = 1
				AND a_t.event IN ('ENTRADA', 'SALIDA')
				GROUP BY
					a_t.r_date,
					a_t.r_hour,
					a_t.event,
					a_t.id_group_camera,
					a_t.group_camera,
					a_t.camera
				ORDER BY
					a_t.r_date,
					a_t.r_hour,
					a_t.id_group_camera,
					a_t.group_camera,
					a_t.camera
			) AS d_a_t
		WHERE
			1 = 1
			AND ( d_a_t.r_date BETWEEN '$P!{fecha_inicio}' AND '$P!{fecha_fin}')
		GROUP BY
			d_a_t.r_date,
			d_a_t.event,
			d_a_t.group_camera
		ORDER BY
			d_a_t.r_date,
			d_a_t.event,
			d_a_t.group_camera
	) AS t_
WHERE
	1 = 1
GROUP BY
	t_.year2,
	t_.month2,
	t_.event,
	t_.group_camera
ORDER BY
	t_.year2,
	t_.month2,
	t_.group_camera]]>
