	<field name="TOTAL" class="java.lang.Long"/>
	<field name="PROMEDIO" class="java.lang.Long"/>
	<variable name="SUM_PROMEDIO" class="java.lang.Integer" calculation="Sum">
		<variableExpression><![CDATA[$F{PROMEDIO}]]></variableExpression>
	</variable>
