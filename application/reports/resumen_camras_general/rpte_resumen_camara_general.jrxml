	<queryString>
		<![CDATA[SELECT
	rc.r_date,
	rc.id_group_camera,
	rc.group_camera,
	rc.event,
	rc.total
FROM
	resumen_camera AS rc
WHERE
	1 = 1
	AND (rc.r_date BETWEEN '$P!{fecha_inicio}' AND '$P!{fecha_fin}')
	$P!{condicion}
ORDER BY
	rc.id_group_camera,
	rc.event,
	rc.r_date]]>
	</queryString>
