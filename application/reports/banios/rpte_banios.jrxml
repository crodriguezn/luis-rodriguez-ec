	<queryString>
		<![CDATA[SELECT
	b.r_date,
	b.r_hour,
	b.id_group_camera,
	b.group_camera,
	b.event,
	b.camera,
	b.total
FROM
	banio AS b
WHERE
	1 = 1
	AND (b.r_date BETWEEN '$P!{fecha_inicio}' AND '$P!{fecha_fin}')
	$P!{condicion}
ORDER BY
	b.id_group_camera,
	b.event,
	b.camera,
	b.r_date,
	b.r_hour]]>
	</queryString>
