	<queryString>
		<![CDATA[SELECT
	a_t.r_date,
	a_t.r_hour,
	a_t.id_group_camera,
	a_t.group_camera,
	a_t.event,
	a_t.camera,
	a_t.total
FROM
	access_terminal AS a_t
WHERE
	1 = 1
	AND (a_t.r_date BETWEEN '$P!{fecha_inicio}' AND '$P!{fecha_fin}')
	$P!{condicion}
ORDER BY
	a_t.id_group_camera,
	a_t.event,
	a_t.camera,
	a_t.r_date,
	a_t.r_hour]]>
	</queryString>
